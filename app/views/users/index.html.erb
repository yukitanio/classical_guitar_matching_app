<h1>ユーザー一覧</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Email</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.email %></td>
      <td><%= link_to 'Show', '#' %></td>
      <td><%= link_to 'Edit', '#' %></td>
      <td><%= link_to 'Destroy', '#' %></td>
    </tr>
  <% end %>
</table>
<%= link_to 'New User', '#' %>
