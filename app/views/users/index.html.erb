<div class="container">
  <h1>ユーザー一覧</h1>
  <div class="mb-3">
    <table class="table table-hover">
      <thead class="thead-default">
        <tr>
          <th>Id</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= user.id %></td>
            <td><%= user.email %></td>
            <td><%= link_to '編集', edit_user_path(user), class: 'btn btn-primary mr-3' %></td>
            <td><%= link_to '削除', '#', class: 'btn btn-danger' %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
